<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('components/head-meta', {name: "Calc Data Details"}); %>

    <link rel="stylesheet" href="/assets/style.css" />

    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />

  <body>
    <%- include('components/navbar'); %>

    <div class="container py-5">

      <% simpleSheme.forEach(image => {%>
        <a href="/assets/sheme/simple_sheme/<%= image %>">
          <img style="max-width: 40vw;" src="/assets/sheme/simple_sheme/<%= image %>">
        </a>
      <%})%>

      <% Object.entries(details).forEach(([name, detail], idx) => { %>
        <% if (idx !== 0) { %>
          <div>
            <h4><%= name %></h4>
            <p><%= detail %></p>
          </div>
        <% } %>
      <% }); %>

      <% sheme.forEach(image => {%>
        <a href="/assets/sheme/sheme/<%= image %>">
          <img style="max-width: 40vw;" src="/assets/sheme/sheme/<%= image %>">
        </a>
      <%})%>

      <h4>Number <%= details['Number'] %></h4>

    </div>

    <%- include('components/footer'); %>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
      crossorigin="anonymous"
    ></script>

  </body>
</html>
